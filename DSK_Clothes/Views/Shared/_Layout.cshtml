<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - DSK CLOTHES🌏</title>
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="icon" href="/Images/logohead.png" type="image/png" sizes="286x286">
    <link rel="stylesheet" href="/Font/fontawesome-free-5.15.4-web/css/all.css">
    <link rel="stylesheet" href="/Font/fontawesome-free-6.2.0-web/css/all.css">

</head>
<body>
    @{
        Html.RenderPartial("HeaderNavBar");
    }
    @RenderBody()
    @{
        Html.RenderPartial("FooterNavBar");
    }
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" crossorigin="anonymous"></script>
    <script src="/Scripts/js/main.js"></script>
</body>
</html>
